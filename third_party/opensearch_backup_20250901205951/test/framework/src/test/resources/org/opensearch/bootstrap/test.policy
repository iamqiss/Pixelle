grant codeBase "${codebase.density-nio}" {
  permission java.net.NetPermission "accessUnixDomainSocket";
};

grant {
  permission java.net.NetPermission "accessUnixDomainSocket";
  permission java.net.SocketPermission "*", "accept,connect";
};
